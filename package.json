{
  "name": "release-cards",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@parcel/packager-css": "^2.0.0-alpha.3",
    "@parcel/transformer-css": "^2.0.0-alpha.3",
    "@parcel/transformer-postcss": "^2.0.0-alpha.3",
    "@types/jest": "^26.0.24",
    "@types/node": "^12.20.16",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "autoprefixer": "^9.8.6",
    "highlight.js": "^11.1.0",
    "marksy": "^8.0.0",
    "parcel": "^2.0.0-beta.2",
    "parcel-namer-without-hash": "0.0.1",
    "playwright": "^1.12.3",
    "postcss": "^7.0.36",
    "prettier": "^2.3.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "serve": "^12.0.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.2.4",
    "typescript": "^4.3.5"
  },
  "scripts": {
    "start": "parcel serve index.html ./src/**/*.tsx",
    "build": "parcel build index.html --public-url '.' --no-optimize --target=node",
    "create-card": "node scripts/create-card.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "targets": {
    "node": {
      "outputFormat": "commonjs",
      "isLibrary": true,
      "includeNodeModules": true,
      "context": "node"
    }
  },
  "engine": {
    "node": "14.16.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
